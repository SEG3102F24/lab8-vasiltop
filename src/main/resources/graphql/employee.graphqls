type Employee {
    id: ID!,
	name: String!,
	dateOfBirth: String!,
	city: String!,
	salary: Int!,
	gender: String,
	email: String!
}

type Query {
    employees: [Employee]
}

type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput): Employee!
}

input CreateEmployeeInput {
    name: String!,
    dateOfBirth: String!,
    city: String!,
    salary: Float!,
    gender: String,
    email: String!
}
